<table border="1">
    <tr>
        <th>Название</th>
        <th>Лектор</th>
        <th>Место</th>
        <th>Дата</th>
    </tr>
    {{#each schedule}}
        <tr>
            <td>
                <a href="/schedule/{{this.id}}">{{this.title}}</a>
            </td>
            <td>{{this.teacher}}</td>
            <td>{{this.place}}</td>
            <td>{{this.date}}</td>
        </tr>
    {{/each}}
</table>

